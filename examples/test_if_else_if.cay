public class TestIfElseIf {
    public static void main() {
        println("=== If-Else-If Tests ===");
        int score = 85;
        if (score >= 90) {
            println("Grade A");
        } else if (score >= 80) {
            println("Grade B");
        } else if (score >= 70) {
            println("Grade C");
        } else if (score >= 60) {
            println("Grade D");
        } else {
            println("Grade F");
        }
        println("All if-else-if tests completed!");
    }
}
