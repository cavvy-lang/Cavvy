public class TestWhileNested {
    public static void main() {
        println("=== Nested While Tests ===");
        int i = 1;
        while (i <= 3) {
            int j = 1;
            while (j <= 3) {
                print("i=");
                print(i);
                print(", j=");
                println(j);
                j = j + 1;
            }
            i = i + 1;
        }
        println("All nested while tests completed!");
    }
}
