public class TestLogicalAnd {
    public static void main() {
        println("=== Logical AND Tests ===");
        bool a = true;
        bool b = false;
        if (a && a) {
            println("true && true is true");
        }
        if (!(a && b)) {
            println("true && false is false");
        }
        if (!(b && a)) {
            println("false && true is false");
        }
        if (!(b && b)) {
            println("false && false is false");
        }
        println("All logical AND tests completed!");
    }
}
