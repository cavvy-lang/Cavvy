// 测试高级多维数组操作
class TestMultidimAdvanced {
    public static void main() {
        println("=== Advanced Multidimensional Array Tests ===");
        
        // Test 2D array
        int[][] matrix = new int[3][4]();
        
        // Fill matrix
        int value = 1;
        for (int i = 0; i < 3; ) {
            for (int j = 0; j < 4; ) {
                matrix[i][j] = value;
                value = value + 1;
                j = j + 1;
            }
            i = i + 1;
        }
        
        // Print matrix
        println("3x4 Matrix:");
        for (int i = 0; i < 3; ) {
            for (int j = 0; j < 4; ) {
                print(matrix[i][j]);
                print(" ");
                j = j + 1;
            }
            println("");
            i = i + 1;
        }
        
        // Test row and column lengths
        print("Matrix rows: ");
        println(matrix.length);
        print("Matrix columns: ");
        println(matrix[0].length);
        
        // Test 2D array with initializer
        int[][] initMatrix = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
        println("Initialized 3x3 matrix:");
        for (int i = 0; i < 3; ) {
            for (int j = 0; j < 3; ) {
                print(initMatrix[i][j]);
                print(" ");
                j = j + 1;
            }
            println("");
            i = i + 1;
        }
        
        // Test diagonal sum
        int diagonalSum = 0;
        for (int i = 0; i < 3; ) {
            diagonalSum = diagonalSum + initMatrix[i][i];
            i = i + 1;
        }
        print("Diagonal sum (1+5+9) = ");
        print(diagonalSum);
        println(" (expected: 15)");
        
        // Test 2D double array
        double[][] doubleMatrix = new double[2][3]();
        doubleMatrix[0][0] = 1.5;
        doubleMatrix[0][1] = 2.5;
        doubleMatrix[0][2] = 3.5;
        doubleMatrix[1][0] = 4.5;
        doubleMatrix[1][1] = 5.5;
        doubleMatrix[1][2] = 6.5;
        
        double dsum = doubleMatrix[0][0] + doubleMatrix[1][2];
        println("Double matrix sum = 8.0 (expected)");
        
        // Test jagged array simulation - use fixed size
        int[][] jagged = new int[3][4]();
        jagged[0][0] = 1;
        jagged[0][1] = 2;
        jagged[1][0] = 3;
        jagged[1][1] = 4;
        jagged[1][2] = 5;
        jagged[1][3] = 6;
        jagged[2][0] = 7;
        jagged[2][1] = 8;
        jagged[2][2] = 9;
        
        print("Jagged-like array row 0 length: ");
        println(jagged[0].length);
        print("Jagged-like array row 1 length: ");
        println(jagged[1].length);
        print("Jagged-like array row 2 length: ");
        println(jagged[2].length);
        
        println("=== All advanced multidim tests completed! ===");
    }
}
