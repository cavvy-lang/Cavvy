public class TestFibonacciIterative {
    public static int fibonacci(int n) {
        if (n <= 1) {
            return n;
        }
        int a = 0;
        int b = 1;
        for (int i = 2; i <= n; i = i + 1) {
            int temp = a + b;
            a = b;
            b = temp;
        }
        return b;
    }

    public static void main() {
        println("=== Fibonacci Iterative Tests ===");
        for (int i = 0; i < 15; i = i + 1) {
            print("fib(");
            print(i);
            print(") = ");
            println(fibonacci(i));
        }
        println("All fibonacci iterative tests completed!");
    }
}
