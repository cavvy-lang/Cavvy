public class TestCompLe {
    public static void main() {
        println("=== Comparison Less Equal Tests ===");
        int a = 10;
        int b = 10;
        int c = 20;
        if (a <= b) {
            println("a <= b is true");
        } else {
            println("a <= b is false");
        }
        if (a <= c) {
            println("a <= c is true");
        } else {
            println("a <= c is false");
        }
        if (c <= a) {
            println("c <= a is true");
        } else {
            println("c <= a is false");
        }
        println("All comparison less equal tests completed!");
    }
}
