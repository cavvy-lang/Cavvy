public class TestPower {
    public static int power(int base, int exp) {
        int result = 1;
        for (int i = 0; i < exp; i = i + 1) {
            result = result * base;
        }
        return result;
    }

    public static void main() {
        println("=== Power Tests ===");
        println(power(2, 10));
        println(power(3, 4));
        println("All power tests completed!");
    }
}
