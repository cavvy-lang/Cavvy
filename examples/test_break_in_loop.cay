public class TestBreakInLoop {
    public static void main() {
        println("=== Break in Loop Tests ===");
        for (int i = 0; i < 10; i = i + 1) {
            if (i == 5) {
                println("Breaking at i = 5");
                break;
            }
            print("i = ");
            println(i);
        }
        println("All break in loop tests completed!");
    }
}
