public class TestForNested {
    public static void main() {
        println("=== Nested For Tests ===");
        for (int i = 1; i <= 3; i = i + 1) {
            for (int j = 1; j <= 3; j = j + 1) {
                print("i=");
                print(i);
                print(", j=");
                println(j);
            }
        }
        println("All nested for tests completed!");
    }
}
