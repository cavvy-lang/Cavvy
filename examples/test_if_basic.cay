public class TestIfBasic {
    public static void main() {
        println("=== Basic If Tests ===");
        int x = 10;
        if (x > 5) {
            println("x is greater than 5");
        }
        if (x < 5) {
            println("x is less than 5");
        } else {
            println("x is not less than 5");
        }
        println("All basic if tests completed!");
    }
}
