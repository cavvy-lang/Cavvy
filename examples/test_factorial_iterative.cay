public class TestFactorialIterative {
    public static long factorial(int n) {
        long result = 1;
        for (int i = 2; i <= n; i = i + 1) {
            result = result * i;
        }
        return result;
    }

    public static void main() {
        println("=== Factorial Iterative Tests ===");
        for (int i = 0; i <= 10; i = i + 1) {
            print(i);
            print("! = ");
            println(factorial(i));
        }
        println("All factorial iterative tests completed!");
    }
}
