public class TestCompLt {
    public static void main() {
        println("=== Comparison Less Than Tests ===");
        int a = 10;
        int b = 20;
        if (a < b) {
            println("a < b is true");
        } else {
            println("a < b is false");
        }
        if (b < a) {
            println("b < a is true");
        } else {
            println("b < a is false");
        }
        println("All comparison less than tests completed!");
    }
}
