// 测试浮点数运算
class TestFloatingPoint {
    public static void main() {
        println("=== Floating Point Tests ===");
        
        // Test float operations
        float f1 = 3.5f;
        float f2 = 2.5f;
        
        float fResult = f1 + f2;
        println("3.5f + 2.5f done");
        
        fResult = f1 - f2;
        println("3.5f - 2.5f done");
        
        fResult = f1 * f2;
        println("3.5f * 2.5f done");
        
        fResult = f1 / f2;
        println("3.5f / 2.5f done");
        
        // Test double operations
        double d1 = 10.5;
        double d2 = 3.5;
        
        double dResult = d1 + d2;
        println("10.5 + 3.5 = 14.0 (expected)");
        
        dResult = d1 - d2;
        println("10.5 - 3.5 = 7.0 (expected)");
        
        dResult = d1 * d2;
        println("10.5 * 3.5 done");
        
        dResult = d1 / d2;
        println("10.5 / 3.5 = 3.0 (expected)");
        
        // Test mixed operations
        dResult = f1 + d2;
        println("3.5f + 3.5 done");
        
        // Test comparison
        bool b = (3.14 < 3.15);
        println("3.14 < 3.15 = true (expected)");
        
        b = (2.5 == 2.5);
        println("2.5 == 2.5 = true (expected)");
        
        // Test float array
        float[] fArr = {1.1f, 2.2f, 3.3f};
        println("Float array sum done");
        
        // Test double array
        double[] dArr = {1.5, 2.5, 3.5};
        double sum = dArr[0] + dArr[1] + dArr[2];
        println("Double array sum = 7.5 (expected)");
        
        // Test negative floats
        float negF = -5.5f;
        println("-5.5f assigned");
        
        double negD = -10.25;
        println("-10.25 assigned");
        
        // Test zero
        float zeroF = 0.0f;
        double zeroD = 0.0;
        println("0.0f assigned");
        println("0.0 assigned");
        
        println("=== All floating point tests completed! ===");
    }
}
