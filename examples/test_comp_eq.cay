public class TestCompEq {
    public static void main() {
        println("=== Comparison Equal Tests ===");
        int a = 10;
        int b = 10;
        int c = 20;
        if (a == b) {
            println("a == b is true");
        } else {
            println("a == b is false");
        }
        if (a == c) {
            println("a == c is true");
        } else {
            println("a == c is false");
        }
        println("All comparison equal tests completed!");
    }
}
